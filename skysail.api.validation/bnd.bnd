Bundle-Version: 0.1.1.${tstamp}
Bundle-Name: SKYSAIL :: api :: validation
Bundle-Description: an incubator bundle for the skysail validation API  (which will be moved \
	to skysail.api once stable) together with a default implementation
-buildpath:  \
	biz.aQute.bnd.annotation;version=2.4,\
	javax.validation.api;version=1.1,\
	org.hibernate.validator;version=5.0,\
	com.springsource.org.junit;version=4.11,\
	org.jboss.logging.jboss-logging;version=3.1,\
	classmate;version=1.0,\
	osgi.core;version=5.0
Export-Package:  \
	io.skysail.api.validation
Private-Package:  \
	io.skysail.api.validation.hibernate,\
	io.skysail.api.validation.hibernate.it
Service-Component:  \
	*
Test-Cases:  \
	io.skysail.api.validation.hibernate.it.ValidationIntegrationTests
	
-runfw: org.apache.felix.framework;version='[4,5)'
-runee: JavaSE-1.8
-runsystemcapabilities: ${native_capability}
-runvm: -ea
-runrepos:  \
	Workspace,\
	Release,\
	Local,\
	Skysail Repository,\
	Bndtools Hub

-resolve.effective: active;skip:="osgi.service"

	
-runrequires:  \
	osgi.identity;filter:='(osgi.identity=org.apache.felix.gogo.shell)',\
	osgi.identity;filter:='(osgi.identity=org.apache.felix.gogo.command)',\
	osgi.identity;filter:='(osgi.identity=org.apache.felix.log)',\
	osgi.identity;filter:='(&(osgi.identity=ch.qos.logback.classic)(version>=1.1.2))',\
	osgi.identity;filter:='(&(osgi.identity=org.apache.felix.scr)(version>=1.8.2))',\
	osgi.identity;filter:='(osgi.identity=skysail.api.validation)'
-resolve: auto
-runbundles:  \
	ch.qos.logback.classic;version='[1.1.2,1.1.3)',\
	ch.qos.logback.core;version='[1.1.2,1.1.3)',\
	classmate;version='[1.0.0,1.0.1)',\
	com.springsource.org.junit;version='[4.11.0,4.11.1)',\
	javax.el;version='[2.2.0,2.2.1)',\
	javax.servlet;version='[3.0.0,3.0.1)',\
	javax.validation.api;version='[1.1.0,1.1.1)',\
	org.apache.felix.configadmin;version='[1.8.0,1.8.1)',\
	org.apache.felix.gogo.command;version='[0.14.0,0.14.1)',\
	org.apache.felix.gogo.runtime;version='[0.12.1,0.12.2)',\
	org.apache.felix.gogo.shell;version='[0.10.0,0.10.1)',\
	org.apache.felix.log;version='[1.0.1,1.0.2)',\
	org.apache.felix.scr;version='[1.8.2,1.8.3)',\
	org.hibernate.validator;version='[5.0.1,5.0.2)',\
	org.jboss.logging.jboss-logging;version='[3.1.2,3.1.3)',\
	osgi.enterprise;version='[4.2.0,4.2.1)',\
	osgi.residential;version='[4.3.0,4.3.1)',\
	skysail.api.validation;version=latest,\
	slf4j.api;version='[1.7.7,1.7.8)'