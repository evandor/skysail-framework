fields(fields) ::= <<

$fields.keys: { key| 

@Field(inputType = InputType.$fields.(key).inputType$, htmlPolicy = HtmlPolicy.$fields.(key).htmlPolicy$)
private String $key$;

public void $fields.(key).setMethodName$(String value) {
    this.$key$ = value;
\}

public String $fields.(key).getMethodName$() {
    return this.$key$;
\}
}$
>>
