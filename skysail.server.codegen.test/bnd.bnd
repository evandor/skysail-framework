### bnd allows only two src-Folders; we will use src-gen instead of test to place the generated code into

Bundle-Version: 0.1.0.${tstamp}
Bundle-Name: SKYSAIL :: server :: codegen :: test
Bundle-Description: test bundle for codegen

testsrc: src-gen
testbin: bin

Service-Component: *

Import-Package: org.osgi.framework,\
	javassist.util.proxy,\
	*

Private-Package:  \
	io.skysail.server.codegen.test.withlist.folders,\
	io.skysail.server.codegen.test.withlist.companies,\
	io.skysail.server.codegen.test.crm,\
	io.skysail.server.codegen.test.withlist,\
	io.skysail.server.codegen.test.withlist.folders,\
	io.skysail.server.codegen.test.crm.contacts,\
	io.skysail.server.codegen.test.simple,\
	io.skysail.server.codegen.test.simple.todos,\
	io.skysail.server.codegen.test.twoentities,\
	io.skysail.server.codegen.test.twoentities.jobs,\
	io.skysail.server.codegen.test.twoentities.schedules,\
	io.skysail.server.codegen.test.withlist.companies

-buildpath:  \
	osgi.core;version=${osgi.version},\
	osgi.cmpn;version=${osgi.version},\
