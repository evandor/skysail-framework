breadcrumbsRow(model,user) ::= <<
  <div class="row" style="background-color:#f5f5f5; height:35px;">
    <div class="col-md-10">
        $breadcrumb(model)$
    </div>
    <div class="col-md-2">
      $if(user.developer)$
        $developerLinks(model)$
      $else$
        <ul class="breadcrumb" style="text-align:right">
          <li>&nbsp;</li>
        </ul>
      $endif$
    </div>
  </div>
>>