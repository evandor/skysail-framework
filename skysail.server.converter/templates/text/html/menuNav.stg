menuNav(linkheader) ::= <<
    $linkheader:{ element | $linkAsButton(element,i)$}$
>>

linkAsButton(linkheader,i) ::= <<
    $if(linkheader.showAsButtonInHtml)$
      <a href="$linkheader.uri$" class="btn btn-default btn-xs" role="button" tabindex="1" style="width:125px;">
      $if(linkheader.image)$
        <span class="glyphicon glyphicon-$linkheader.image$" aria-hidden="true"></span>
      $endif$
      $linkheader.title$
      </a>&nbsp;
    $endif$
>>