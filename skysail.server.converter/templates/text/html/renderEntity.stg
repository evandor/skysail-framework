renderEntity(user, source, model) ::= <<
  <table class="table">
    $model.formfields: {field | $renderRow(source.entity.entity, field, first(model.data))$}$
  </table>
>>

renderRow(entity, field, data) ::= <<
  <tr><th>$field.name$</th><td>&nbsp;<td>$data.(field.name)$</td><td>$handleDates(entity,field)$</td></tr>
>>

handleDates(entity, field) ::= <<
    $if(field.dateType)$
    <script>document.write(moment("$entity.(field.name)$").fromNow());</script>
    $endif$
>>