renderEntity(user, model) ::= <<
  <table class="table">
    $model.formfields: {field | $renderRow(field, first(model.data))$}$
  </table>
>>

renderRow(field, data) ::= <<
  <tr><th>$field.name$</th><td>&nbsp;<td>$data.(field.name)$</td><td>$handleDates(data,field)$</td></tr>
>>

handleDates(data, field) ::= <<
    $if(field.dateType)$
    <script>document.write(moment("$data.(field.name)$").fromNow());</script>
    $endif$
>>