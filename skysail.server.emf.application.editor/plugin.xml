<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated skysailApplication -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="skysailApplication.presentation.SkysailApplicationModelWizardID"
            name="%_UI_SkysailApplicationModelWizard_label"
            class="skysailApplication.presentation.SkysailApplicationModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SkysailApplicationModelFile.gif">
         <description>%_UI_SkysailApplicationModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated skysailApplication -->
      <editor
            id="skysailApplication.presentation.SkysailApplicationEditorID"
            name="%_UI_SkysailApplicationEditor_label"
            icon="icons/full/obj16/SkysailApplicationModelFile.gif"
            extensions="skysailapplication"
            class="skysailApplication.presentation.SkysailApplicationEditor"
            contributorClass="skysailApplication.presentation.SkysailApplicationActionBarContributor">
      </editor>
   </extension>

</plugin>
