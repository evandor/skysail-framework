renderTable(resource, source, fields) ::= <<
  <table>
      $source:{ bundleMessages | $renderRow(bundleMessages,i)$}$
  </table>
>>

renderRow(row,i) ::= <<
  <tr>
    <td>
      <h4>$i$: $row.name$:</h4>
      <ul>
        $row.("messages"): {msg | <li>$msg$ [<a href='$msg$/'>edit</a>]</li>}$
      </ul>
    </td>
  </tr>
>>